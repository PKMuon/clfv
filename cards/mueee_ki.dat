#!/usr/bin/env mg5_aMC

set nb_core 2

import model sm-zp

generate mu+ e- > e+ e-
output $SUB:workdir
launch

done

set MZp = $SUB:zp_mass
set WZp = $SUB:zp_width
set nevents = $SUB:nevent
set lpp1 = 0
set lpp2 = 0
set ptl = -1.0
set etal = -1.0
set drll = -1.0
set ebeam1 = $SUB:muon_energy
set ebeam2 = $SUB:electron_energy
set pdlabel = none
set use_syst = False
done

!$SUB:make_mueee_distributions $SUB:workdir/Events/run_01/unweighted_events.lhe.gz $SUB:mueee_kinematics
